<serviceGroup>

    <service name="CAAdminService" scope="transportsession">
        <transports>
            <transport>https</transport>
        </transports>
        <schema schemaNamespace="http://org.apache.axis2/xsd" elementFormDefaultQualified="true"/>
        <description>CA administration related functionality</description>
        <parameter name="ServiceClass">org.wso2.carbon.identity.certificateauthority.CAAdminService</parameter>
        <!--<parameter name="AuthorizationAction" locked="true">/permission/admin/manage</parameter>-->
        <parameter name="AuthorizationAction" locked="true">/permission/admin/login</parameter>
        <parameter name="adminService" locked="true">true</parameter>
        <parameter name="hiddenService" locked="true">false</parameter>
    </service>


    <service name="CAClientService" scope="transportsession">
        <transports>
            <transport>https</transport>
        </transports>
        <schema schemaNamespace="http://org.apache.axis2/xsd" elementFormDefaultQualified="true"/>
        <description>CA client services</description>
        <parameter name="ServiceClass">org.wso2.carbon.identity.certificateauthority.CAClientService</parameter>
        <parameter name="AuthorizationAction" locked="true">/permission/admin/login</parameter>
        <parameter name="adminService" locked="true">true</parameter>
        <parameter name="hiddenService" locked="true">false</parameter>
    </service>

</serviceGroup>